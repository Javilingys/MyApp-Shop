@model IReadOnlyList<ProductDto>
@{
    ViewData["Title"] = "Products";
}

<div class="container">
    <ul>
        @{ foreach (var item in Model)
            {
                <li>Name: @item.Name Description: @item.Description Price: @item.Price</li>
            }
        }
    </ul>
</div>
